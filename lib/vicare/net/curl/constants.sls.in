(library (vicare net curl constants)
  (export

    ;; version numbers
    LIBCURL_VERSION_MAJOR		LIBCURL_VERSION_MINOR
    LIBCURL_VERSION_PATCH		LIBCURL_VERSION_NUM
    LIBCURL_COPYRIGHT			LIBCURL_VERSION
    LIBCURL_TIMESTAMP

    ;; miscellaneous constants
    CURL_SOCKET_BAD
    CURL_MAX_WRITE_SIZE
    CURL_MAX_HTTP_HEADER
    CURL_WRITEFUNC_PAUSE

    ;;
    HTTPPOST_FILENAME			HTTPPOST_READFILE
    HTTPPOST_PTRNAME			HTTPPOST_PTRCONTENTS
    HTTPPOST_BUFFER			HTTPPOST_PTRBUFFER
    HTTPPOST_CALLBACK

    ;; enumeration of file types
    CURLFILETYPE_FILE			CURLFILETYPE_DIRECTORY
    CURLFILETYPE_SYMLINK		CURLFILETYPE_DEVICE_BLOCK
    CURLFILETYPE_DEVICE_CHAR		CURLFILETYPE_NAMEDPIPE
    CURLFILETYPE_SOCKET			CURLFILETYPE_DOOR
    CURLFILETYPE_UNKNOWN

    ;;
    CURLFINFOFLAG_KNOWN_FILENAME	CURLFINFOFLAG_KNOWN_FILETYPE
    CURLFINFOFLAG_KNOWN_TIME		CURLFINFOFLAG_KNOWN_PERM
    CURLFINFOFLAG_KNOWN_UID		CURLFINFOFLAG_KNOWN_GID
    CURLFINFOFLAG_KNOWN_SIZE		CURLFINFOFLAG_KNOWN_HLINKCOUNT

    ;; return codes for CURLOPT_CHUNK_BGN_FUNCTION
    CURL_CHUNK_BGN_FUNC_OK		CURL_CHUNK_BGN_FUNC_FAIL
    CURL_CHUNK_BGN_FUNC_SKIP

    )
  (import (rename (only (vicare syntactic-extensions)
			define-inline-constant)
		  (define-inline-constant	defc)))


;;;; constants

;; version numbers
(defc LIBCURL_VERSION_MAJOR	@VALUEOF_LIBCURL_VERSION_MAJOR@)
(defc LIBCURL_VERSION_MINOR	@VALUEOF_LIBCURL_VERSION_MINOR@)
(defc LIBCURL_VERSION_PATCH	@VALUEOF_LIBCURL_VERSION_PATCH@)
(defc LIBCURL_VERSION_NUM	@VALUEOF_LIBCURL_VERSION_NUM@)

(defc LIBCURL_COPYRIGHT		"@VALUEOF_LIBCURL_COPYRIGHT@")
(defc LIBCURL_VERSION		"@VALUEOF_LIBCURL_VERSION@")
(defc LIBCURL_TIMESTAMP		"@VALUEOF_LIBCURL_TIMESTAMP@")

;; miscellaneous constants
(defc CURL_SOCKET_BAD		@VALUEOF_CURL_SOCKET_BAD@)
(defc CURL_MAX_WRITE_SIZE	@VALUEOF_CURL_MAX_WRITE_SIZE@)
(defc CURL_MAX_HTTP_HEADER	@VALUEOF_CURL_MAX_HTTP_HEADER@)
(defc CURL_WRITEFUNC_PAUSE	@VALUEOF_CURL_WRITEFUNC_PAUSE@)

;;
(defc HTTPPOST_FILENAME		@VALUEOF_HTTPPOST_FILENAME@)
(defc HTTPPOST_READFILE		@VALUEOF_HTTPPOST_READFILE@)
(defc HTTPPOST_PTRNAME		@VALUEOF_HTTPPOST_PTRNAME@)
(defc HTTPPOST_PTRCONTENTS	@VALUEOF_HTTPPOST_PTRCONTENTS@)
(defc HTTPPOST_BUFFER		@VALUEOF_HTTPPOST_BUFFER@)
(defc HTTPPOST_PTRBUFFER	@VALUEOF_HTTPPOST_PTRBUFFER@)
(defc HTTPPOST_CALLBACK		@VALUEOF_HTTPPOST_CALLBACK@)

;; enumeration of file types
(defc CURLFILETYPE_FILE			@VALUEOF_CURLFILETYPE_FILE@)
(defc CURLFILETYPE_DIRECTORY		@VALUEOF_CURLFILETYPE_DIRECTORY@)
(defc CURLFILETYPE_SYMLINK		@VALUEOF_CURLFILETYPE_SYMLINK@)
(defc CURLFILETYPE_DEVICE_BLOCK		@VALUEOF_CURLFILETYPE_DEVICE_BLOCK@)
(defc CURLFILETYPE_DEVICE_CHAR		@VALUEOF_CURLFILETYPE_DEVICE_CHAR@)
(defc CURLFILETYPE_NAMEDPIPE		@VALUEOF_CURLFILETYPE_NAMEDPIPE@)
(defc CURLFILETYPE_SOCKET		@VALUEOF_CURLFILETYPE_SOCKET@)
