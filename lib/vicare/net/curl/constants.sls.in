(defc CURLFINFOFLAG_KNOWN_GID		@VALUEOF_CURLFINFOFLAG_KNOWN_GID@)
(defc CURLFINFOFLAG_KNOWN_SIZE		@VALUEOF_CURLFINFOFLAG_KNOWN_SIZE@)
(defc CURLFINFOFLAG_KNOWN_HLINKCOUNT	@VALUEOF_CURLFINFOFLAG_KNOWN_HLINKCOUNT@)

;; return codes for CURLOPT_CHUNK_BGN_FUNCTION
(defc CURL_CHUNK_BGN_FUNC_OK		@VALUEOF_CURL_CHUNK_BGN_FUNC_OK@)
(defc CURL_CHUNK_BGN_FUNC_FAIL		@VALUEOF_CURL_CHUNK_BGN_FUNC_FAIL@)
(defc CURL_CHUNK_BGN_FUNC_SKIP		@VALUEOF_CURL_CHUNK_BGN_FUNC_SKIP@)


